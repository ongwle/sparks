<h1>Listing project_galleries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_galleries.each do |project_gallery| %>
      <tr>
        <td><%= link_to 'Show', project_gallery %></td>
        <td><%= link_to 'Edit', edit_project_gallery_path(project_gallery) %></td>
        <td><%= link_to 'Destroy', project_gallery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project gallery', new_project_gallery_path %>
